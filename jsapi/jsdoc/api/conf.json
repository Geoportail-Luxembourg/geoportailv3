{
    "opts": {
        "recurse": true,
        "template": "jsapi/jsdoc/api/template"
    },
    "tags": {
        "allowUnknownTags": true
    },
    "source": {
        "includePattern": ".+\\.js(doc)?$",
        "include": [
            "jsapi/src/",
            "jsapi/externs/luxx.js"
        ]
    },
    "plugins": [
        "node_modules/jsdoc/plugins/markdown"
    ],
    "markdown": {
        "parser": "gfm"
    },
    "stability": {
      "levels": [
        "deprecated",
        "experimental",
        "unstable",
        "stable",
        "frozen",
        "locked"
      ]
    },
    "templates": {
        "cleverLinks": true,
        "monospaceLinks": true,
        "default": {
            "outputSourceFiles": false
        },
        "applicationName": "Luxembourg API"
    },
    "jsVersion": 180
}
